void setup() {
  Serial.begin(9600);
  strip.begin();
}

void loop() {
  // delay(10);
  if( (millis() % 5000) == 0) {
    for (int i = 0; i < numSquares; i++) {
      for (int j = 0; j < numSquares; j++) {
        arr2[i] += arrTrans5[j][i] * arr1[j];
      }
    }
    for (int i = 0; i < numSquares; i++) {
      arr1[i] = arr2[i];
      arr2[i] = 0;
      if (arr1[i] == 0) {
        strip.setPixelColor(i,127,127,127);
      }
      else if (arr1[i] == 1) {
        strip.setPixelColor(i,127,0,0);
      }
      else if (arr1[i] == 2) {
        strip.setPixelColor(i,0,127,0);
      }
      else if (arr1[i] == 3) {
        strip.setPixelColor(i,0,0,127);
      }
      else if (arr1[i] == 4) {
        strip.setPixelColor(i,127,127,0);
      }
      else if (arr1[i] == 5) {
        strip.setPixelColor(i,0,127,127);
      }
      Serial.print(arr1[i]);
      Serial.print(' ');
    }
    Serial.println(' ');
    strip.show();
    // Serial.println(millis());
  }
}

